{% extends 'RegistroAcademicoBundle:Default:layout.html.twig' %}

{% block content -%}
    <h1>Aspirante list</h1>

    <table class="table table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Foto</th>
                <th>Primerapellido</th>
                <th>Segundoapellido</th>
                <th>Nombres</th>
                <th>Especialidad</th>
                <th>Direccion</th>
                <th>Telefono</th>
                <th>Fechanac</th>
                <th>Lugarnac</th>
                <th>Sexo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('expediente_aspirante_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.foto }}</td>
                <td>{{ entity.primerapellido }}</td>
                <td>{{ entity.segundoapellido }}</td>
                <td>{{ entity.nombres }}</td>
                <td>{{ entity.especialidad }}</td>
                <td>{{ entity.direccion }}</td>
                <td>{{ entity.telefono }}</td>
                <td>{% if entity.fechanac %}{{ entity.fechanac|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.lugarnac }}</td>
                <td>{{ entity.sexo }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('expediente_aspirante_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('expediente_aspirante_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('expediente_aspirante_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
