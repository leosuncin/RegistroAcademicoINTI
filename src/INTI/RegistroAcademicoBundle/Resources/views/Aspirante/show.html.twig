{% extends 'RegistroAcademicoBundle:Default:layout.html.twig' %}

{% block breadcrumb %}
<li><span class="divider">&gt;</span><a href="{{ path('index') }}">Inicio</a></li>
<li><span class="divider">&gt;</span><a href="{{ path('aspirante_index') }}">Aspirantes</a></li>
<li class="active"><span class="divider">&gt;</span> Consultar</li>
{% endblock %}

{% block content %}
{% for flashMessage in app.session.flashbag.get('confirm') %}
<div class="alert alert-success">
    {{ flashMessage }}
</div>
{% endfor %}
<ul class="nav nav-tabs">
    <li class="active"><a href="#tabInformacion" data-toggle="tab">Información básica</a></li>
    <li><a href="#tabEncargado" data-toggle="tab">Encargado</a></li>
</ul>
<ul id="tabContent" class="tab-content">
    <li id="tabInformacion" class="tab-pane fade in active">
        <table class="record_properties">
            <tbody>
                <tr>
                    <th>Foto</th>
                    <td><img src="data:image/jpg;base64,{{ entity.foto|default('') }}" alt="Fotografia" class="profile_id img-polaroid" /></td>
                </tr>
                <tr>
                    <th>Nombre completo</th>
                    <td>{{ entity.primerapellido~' '~entity.segundoapellido~', '~entity.nombres }}</td>
                </tr>
                <tr>
                    <th>Especialidad</th>
                    <td>{{ entity.especialidad.nombre }}</td>
                </tr>
                <tr>
                    <th>Direccion</th>
                    <td>{{ entity.direccion }}</td>
                </tr>
                <tr>
                    <th>Telefono</th>
                    <td>{{ entity.telefono }}</td>
                </tr>
                <tr>
                    <th>Fechanac</th>
                    <td>{{ entity.fechanac|date('d-m-Y') }}</td>
                </tr>
                <tr>
                    <th>Lugarnac</th>
                    <td>{{ entity.lugarnac }}</td>
                </tr>
                <tr>
                    <th>Sexo</th>
                    <td>{{ entity.sexo }}</td>
                </tr>
            </tbody>
        </table>
        <div class="form-actions">
          <a href="{{ path('aspirante_edit', { 'id': entity.id }) }}" class="btn btn-info">Modificar</a>
          <a href="{{ path('aspirante_erase', { 'id': entity.id }) }}" class="btn btn-danger">Eliminar</a>
          <a href="{{ path('aspirante_index') }}" class="btn">Regresar</a>
        </div>
    </li>
    <li id="tabEncargado" class="tab-pane fade">
        <table class="record_properties">
            <tr>
                <th>Nombre completo</th>
                <td>{{ entity.encargado.nombre }}</td>
            </tr>
            <tr>
                <th>Parentesco</th>
                <td>{{ entity.encargado.parentesco }}</td>
            </tr>
            <tr>
                <th>DUI</th>
                <td>{{ entity.encargado.dui }}</td>
            </tr>
            <tr>
                <th>Teléfono</th>
                <td>{{ entity.encargado.telefono }}</td>
            </tr>
        </table>
    </li>
</ul>
{% endblock %}
